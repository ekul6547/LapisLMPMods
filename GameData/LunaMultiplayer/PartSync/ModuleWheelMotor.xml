        </FieldDefinition>
        <!--Motor Inverted. Not needed.-->
        <FieldDefinition>
            <FieldName>motorInverted</FieldName>
            <Ignore>true</Ignore>
        </FieldDefinition>
    </Fields>
</ModuleDefinition>
